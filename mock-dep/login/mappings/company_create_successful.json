{
  "request": {
    "method": "POST",
    "url": "/company",
    "headers": {
      "Content-Type": {
        "equalTo": "application/json"
      }
    },
    "bodyPatterns": [
      {
        "equalToJson": "{ 'companyName': 'Good Company', 'companyId': '123456789' }"
      }
    ]
  },
  "response": {
    "status": 201,
    "headers": {
      "Content-Type": "application/json; charset=UTF-8",
      "Cache-Control": "no-cache"
    },
    "jsonBody": {
      "error": "",
      "companyId": "123456789"
    }
  }
}